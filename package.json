  "devDependencies": {
    "@parcel/transformer-sass": "^2.0.0",
    "autoprefixer": "^10.4.0",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.0.0",
    "postcss-preset-env": "^6.7.0",
    "rimraf": "^3.0.2",
    "sharp": "^0.29.1"
